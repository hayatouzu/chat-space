.chat-main
  .main-header
    .current-group
      .current-group__name
        = @group.name
        %ul.member-list
          %p Member :
          %li.member-list__member
            - @group.users.each do |user|
              = user.name
    .main-header__edit-btn
      = link_to edit_group_path(current_user) do
        %p Edit